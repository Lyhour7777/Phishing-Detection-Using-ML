    "from urllib.parse import urlparse\n",
    "import ipaddress\n",
    "\n",
    "\n",
    "def having_ip(url):\n",
    "    try:\n",
    "        host = urlparse(url).hostname\n",
    "        ipaddress.ip_address(host)\n",
    "        return 1\n",
    "    except:\n",
    "        return 0\n",
    "\n",
    "having_ip('https://aidasdir.github.io/MyEtherWallet/')"
   ]
  }
